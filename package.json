{
  "name": "b2b-nextjs-app-router-demo-app",
  "description": "Invotastic for Business is a multi-tenant demo app powered by Wristband. It is using NextJS with app routing.",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -p 6001",
