    "moment": "^2.30.1",
    "next": "^14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
